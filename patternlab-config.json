{
  "paths": {
    "source": {
      "root": "./source/",
      "patterns": "./source/_patterns/",
      "data": "./source/_data/",
      "meta": "./source/_meta/",
      "annotations": "./source/_annotations/",
      "styleguide": "./source/styleguide/dist/",
      "patternlabFiles": "./node_modules/styleguidekit-mustache-default/views/",
      "js": "./source/js",
      "images": "./source/images",
      "icons": "./source/images/icons",
      "fonts": "./source/fonts",
      "css": "./source/styles/",
      "bundleCss": "./source/styles/00-dist"
    },
    "public": {
      "root": "./public/",
      "patterns": "./public/patterns/",
      "data": "./public/styleguide/data/",
      "annotations": "./public/annotations/",
      "styleguide": "./public/styleguide/",
      "js": "./public/js",
      "images": "./public/images",
      "icons": "./public/images/icons",
      "fonts": "./public/fonts",
      "css": "./public/css"
    }
  },
  "styleGuideExcludes": [],
  "defaultPattern": "principles-pages-welcome",
  "defaultShowPatternInfo": false,
  "cleanPublic": true,
  "patternExtension": "mustache",
  "ignored-extensions": [
    "scss",
    "DS_Store",
    "less"
  ],
  "ignored-directories": [
    "scss"
  ],
  "debug": false,
  "ishControlsHide": {
    "s": false,
    "m": false,
    "l": false,
    "full": true,
    "random": true,
    "disco": true,
    "hay": false,
    "mqs": false,
    "find": false,
    "views-all": true,
    "views-annotations": false,
    "views-code": false,
    "views-new": false,
    "tools-all": false,
    "tools-docs": false
  },
  "plugins": {
    "plugin-node-tab": {
      "enabled": true,
      "initialized": false
    }
  },
  "ishMinimum": "150",
  "ishMaximum": "1215",
  "patternStateCascade": [
    "inprogress",
    "inreview",
    "complete"
  ],
  "patternStates": {},
  "patternExportPatternPartials": [],
  "patternExportDirectory": "./pattern_exports/",
  "cacheBust": true,
  "starterkitSubDir": "dist",
  "outputFileSuffixes": {
    "rendered": ".rendered",
    "rawTemplate": "",
    "markupOnly": ".markup-only"
  },
  "cleanOutputHtml": true
}
